  <object class="wxDialog" name="ID_FILEEXISTSDLG">
    <style>wxCAPTION|wxSYSTEM_MENU|wxCLOSE_BOX</style>
    <exstyle>wxWS_EX_BLOCK_EVENTS</exstyle>
    <title>Target file already exists</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <flag>wxALL</flag>
            <border>5</border>
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText" name="">
                  <label>The target file already exists.\nPlease choose an action.</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="">
                  <label>Source file:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="ID_FILE2_NAME">
                  <label>&lt;file&gt;</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
                  <object class="sizeritem">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                    <border>10</border>
                    <object class="wxStaticBitmap" name="ID_FILE2_ICON">
                      <bitmap>32x32/file.png</bitmap>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxALIGN_CENTRE_VERTICAL</flag>
                    <border>5</border>
                    <object class="wxBoxSizer">
                      <orient>wxVERTICAL</orient>
                      <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxStaticText" name="ID_FILE2_SIZE">
                          <label>&lt;size&gt;</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxStaticText" name="ID_FILE2_TIME">
                          <label>&lt;date/time&gt;</label>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Target file:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText" name="ID_FILE1_NAME">
                  <label>&lt;file&gt;</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
                  <object class="sizeritem">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
                    <border>10</border>
                    <object class="wxStaticBitmap" name="ID_FILE1_ICON">
                      <bitmap>32x32/file.png</bitmap>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxALIGN_CENTRE_VERTICAL</flag>
                    <border>5</border>
                    <object class="wxBoxSizer">
                      <orient>wxVERTICAL</orient>
                      <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxStaticText" name="ID_FILE1_SIZE">
                          <label>&lt;size&gt;</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxStaticText" name="ID_FILE1_TIME">
                          <label>&lt;date/time&gt;</label>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
              <cols>1</cols>
              <vgap>5</vgap>
            </object>
          </object>
          <object class="spacer">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <size>3,4</size>
          </object>
          <object class="sizeritem">
            <flag>wxALL|wxALIGN_TOP</flag>
            <border>5</border>
            <object class="wxBoxSizer">
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <flag>wxTOP|wxBOTTOM|wxLEFT|wxGROW</flag>
                <border>5</border>
                <object class="wxStaticBoxSizer">
                  <orient>wxVERTICAL</orient>
                  <label>Action:</label>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION1">
                      <label>&amp;Overwrite</label>
                      <value>1</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION2">
                      <label>Overwrite &amp;if source newer</label>
                      <value>0</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION7">
                      <label>Overwrite if different size</label>
                      <value>0</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION6">
                      <label>Overwrite if different size or source newer</label>
                      <value>0</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION3">
                      <label>&amp;Resume</label>
                      <value>0</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxLEFT|wxRIGHT|wxTOP</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION4">
                      <label>Re&amp;name</label>
                      <value>0</value>
                    </object>
                  </object>
                  <object class="sizeritem">
                    <flag>wxALL</flag>
                    <border>5</border>
                    <object class="wxRadioButton" name="ID_ACTION5">
                      <label>&amp;Skip</label>
                      <value>0</value>
                    </object>
                  </object>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxTOP|wxLEFT</flag>
                <border>5</border>
                <object class="wxCheckBox" name="ID_ALWAYS">
                  <label>&amp;Always use this action</label>
                  <checked>0</checked>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxLEFT</flag>
                <border>5</border>
                <object class="wxBoxSizer">
                  <orient>wxHORIZONTAL</orient>
                  <object class="spacer">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                    <border>4</border>
                    <size>3,4</size>
                  </object>
                  <object class="sizeritem">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                    <border>5</border>
                    <object class="wxBoxSizer">
                      <orient>wxVERTICAL</orient>
                      <object class="sizeritem">
                        <flag>wxRIGHT|wxTOP</flag>
                        <border>5</border>
                        <object class="wxCheckBox" name="ID_QUEUEONLY">
                          <label>Apply to &amp;current queue only</label>
                          <checked>0</checked>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxRIGHT|wxTOP|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxCheckBox" name="ID_UPDOWNONLY">
                          <label>&lt;updownonly&gt;</label>
                          <checked>0</checked>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
          </object>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxALIGN_CENTRE_HORIZONTAL</flag>
        <border>5</border>
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL|wxGROW</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_OK">
              <label>OK</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL|wxGROW</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_CANCEL">
              <label>Cancel</label>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
