          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxButton" name="wxID_OK">
                  <label>OK</label>
                  <default>1</default>
                </object>
                <flag>wxRIGHT|wxALIGN_CENTRE_VERTICAL|wxGROW</flag>
                <border>5</border>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="wxID_CANCEL">
                  <label>Cancel</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL|wxGROW</flag>
              </object>
            </object>
            <flag>wxALIGN_RIGHT</flag>
          </object>
        </object>
        <flag>wxALL</flag>
        <border>5</border>
      </object>
    </object>
    <title>No program associated with filetype</title>
  </object>
  <object class="wxDialog" name="ID_SEARCH">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Search &amp;directory:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL|wxRIGHT</flag>
                <border>5</border>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_PATH"/>
                <option>1</option>
                <flag>wxALIGN_CENTRE_VERTICAL|wxGROW</flag>
              </object>
              <orient>wxHORIZONTAL</orient>
            </object>
            <flag>wxGROW</flag>
          </object>
          <object class="sizeritem">
            <object class="wxButton" name="ID_START">
              <label>&amp;Search</label>
              <default>1</default>
            </object>
          </object>
          <object class="spacer"/>
          <object class="sizeritem">
            <object class="wxButton" name="ID_STOP">
              <label>S&amp;top</label>
            </object>
          </object>
          <cols>2</cols>
          <vgap>5</vgap>
          <hgap>5</hgap>
          <growablecols>0</growablecols>
        </object>
        <flag>wxALL|wxGROW</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Search &amp;conditions:</label>
            </object>
            <flag>wxALIGN_CENTRE_VERTICAL</flag>
          </object>
          <object class="sizeritem">
            <object class="wxChoice" name="ID_MATCHTYPE">
              <content>
                <item>Match all of the following</item>
                <item>Match any of the following</item>
                <item>Match none of the following</item>
              </content>
              <selection>0</selection>
            </object>
            <flag>wxALIGN_CENTRE_VERTICAL</flag>
          </object>
          <cols>2</cols>
          <rows>1</rows>
          <hgap>5</hgap>
        </object>
        <flag>wxLEFT|wxRIGHT|wxTOP</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxScrolledWindow" name="ID_CONDITIONS">
              <size>350,120</size>
            </object>
            <flag>wxGROW</flag>
          </object>
          <object class="sizeritem">
            <object class="wxCheckBox" name="ID_CASE">
              <label>Conditions are c&amp;ase sensitive</label>
            </object>
          </object>
          <cols>1</cols>
          <vgap>5</vgap>
          <growablecols>0</growablecols>
        </object>
        <flag>wxALL|wxGROW</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxStaticLine"/>
        <flag>wxALL|wxGROW</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxStaticText">
          <label>Results:</label>
        </object>
        <flag>wxLEFT|wxRIGHT</flag>
        <border>5</border>
      </object>
      <object class="sizeritem">
        <object class="wxListCtrl" name="ID_RESULTS">
          <style>wxLC_REPORT</style>
        </object>
        <option>1</option>
        <flag>wxALL|wxGROW</flag>
        <border>5</border>
      </object>
    </object>
    <title>Remote file search</title>
    <style>wxDEFAULT_DIALOG_STYLE|wxRESIZE_BORDER</style>
  </object>
  <object class="wxDialog" name="ID_SEARCH_DOWNLOAD">
    <title>Download search results</title>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxStaticText" name="ID_DESC">
              <label>&lt;&lt;Download some files and directories&gt;</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>&amp;Download to:</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_LOCALPATH"/>
                <option>1</option>
                <flag>wxALIGN_CENTRE_VERTICAL|wxRIGHT</flag>
                <border>5</border>
              </object>
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <object class="wxButton" name="ID_BROWSE">
                  <label>&amp;Browse...</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
            </object>
            <flag>wxGROW</flag>
          </object>
          <object class="spacer"/>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Remote path handling:</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_PATHS_KEEP">
              <label>&amp;Keep remote path structure relative to search root</label>
              <value>1</value>
              <style>wxRB_GROUP</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_PATHS_FLATTEN">
              <label>&amp;Flatten remote paths, download all contained files directly into target directory</label>
            </object>
          </object>
          <object class="spacer"/>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Queueing:</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_QUEUE_START">
              <label>&amp;Start transfer immediately</label>
              <value>1</value>
              <style>wxRB_GROUP</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_QUEUE_ADD">
              <label>Add files to &amp;queue only</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticLine"/>
            <flag>wxGROW</flag>
          </object>
          <object class="sizeritem">
            <object class="wxGridSizer">
              <object class="sizeritem">
                <object class="wxButton" name="wxID_OK">
                  <label>OK</label>
                  <default>1</default>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="wxID_CANCEL">
                  <label>Cancel</label>
                </object>
              </object>
              <cols>2</cols>
              <rows>1</rows>
              <hgap>5</hgap>
            </object>
            <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
          </object>
          <cols>1</cols>
          <rows>2</rows>
          <vgap>5</vgap>
          <hgap>5</hgap>
        </object>
        <flag>wxALL</flag>
        <border>5</border>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="ID_DELETE_SYMLINK">
    <title>Deleting symbolic link</title>
    <object class="wxBoxSizer">
      <orient>wxHORIZONTAL</orient>
      <object class="sizeritem">
        <object class="wxFlexGridSizer">
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>You have selected at least one symbolic link.</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>If the symlink points to a directory, FileZilla can either delete the symbolic link or remove the contents of the linked directory.</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Please select an action:</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_DELE">
              <label>&amp;Delete symlink</label>
              <value>1</value>
              <style>wxRB_GROUP</style>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxRadioButton" name="ID_RECURSE">
              <label>&amp;Follow symlink, delete target directory contents</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticText">
              <label>Symlinks to files are not affected by this choice, such symlinks are always deleted.</label>
            </object>
          </object>
          <object class="sizeritem">
            <object class="wxStaticLine"/>
            <flag>wxGROW</flag>
          </object>
          <object class="sizeritem">
            <object class="wxGridSizer">
              <object class="sizeritem">
                <object class="wxButton" name="wxID_OK">
                  <label>OK</label>
                  <default>1</default>
                </object>
                <flag>wxALIGN_CENTER_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxButton" name="wxID_CANCEL">
                  <label>Cancel</label>
                </object>
              </object>
              <cols>2</cols>
              <hgap>5</hgap>
            </object>
            <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
          </object>
          <cols>1</cols>
          <vgap>5</vgap>
        </object>
        <flag>wxALL</flag>
        <border>5</border>
      </object>
    </object>
  </object>
</resource>