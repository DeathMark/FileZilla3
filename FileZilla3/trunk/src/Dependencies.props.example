<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <wxwidgets_include>x:\xample\wxwidgets\include;x:\xample\wxwidgets\include\msvc</wxwidgets_include>
    <wxwidgets_lib>x:\xample\wxwidgets\lib\vc_lib</wxwidgets_lib>
    <gnutls_include>x:\xample\gnutls-3.1.10-win\include</gnutls_include>
    <gnutls_lib>x:\xample\gnutls-3.1.10-win\bin</gnutls_lib>
    <libidn_include>x:\xample\libidn-1.19\win32\include</libidn_include>
    <libidn_lib>x:\xample\libidn-1.19\win32\lib</libidn_lib>
    <sqlite3_include>x:\xample\sqlite-amalgamation-3080200</sqlite3_include>
    <sqlite3_lib>x:\xample\sqlite-amalgamation-3080200</sqlite3_lib>
  </PropertyGroup>
  <PropertyGroup>
    <IncludePath>$(wxwidgets_include);$(gnutls_include);$(libidn_include);$(sqlite3_include);$(IncludePath)</IncludePath>
    <LibraryPath>$(wxwidgets_lib);$(gnutls_lib);$(libidn_lib);$(sqlite3_lib);$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="wxwidgets_include">
      <Value>$(wxwidgets_include)</Value>
    </BuildMacro>
    <BuildMacro Include="wxwidgets_lib">
      <Value>$(wxwidgets_lib)</Value>
    </BuildMacro>
    <BuildMacro Include="gnutls_include">
      <Value>$(gnutls_include)</Value>
    </BuildMacro>
    <BuildMacro Include="gnutls_lib">
      <Value>$(gnutls_lib)</Value>
    </BuildMacro>
    <BuildMacro Include="libidn_include">
      <Value>$(libidn_include)</Value>
    </BuildMacro>
    <BuildMacro Include="libidn_lib">
      <Value>$(libidn_lib)</Value>
    </BuildMacro>
    <BuildMacro Include="sqlite3_include">
      <Value>$(sqlite3_include)</Value>
    </BuildMacro>
    <BuildMacro Include="sqlite3_lib">
      <Value>$(sqlite3_lib)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>