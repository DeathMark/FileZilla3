      <label>C&amp;lear all</label>
    </object>
  </object>
  <object class="wxMenu" name="ID_MENU_DEBUG">
    <object class="wxMenuItem" name="ID_CLEARCACHE_LAYOUT">
      <label>Clear &amp;layout cache</label>
    </object>
    <object class="separator"/>
    <object class="wxMenuItem" name="ID_CRASH">
      <label>&amp;Crash</label>
      <help>Crashes FileZilla instantly. Only used to debug the exception handler</help>
    </object>
  </object>
  <object class="wxMenu" name="ID_MENU_QUEUE_FAILED">
    <object class="wxMenuItem" name="ID_REMOVEALL">
      <label>Remove &amp;all</label>
    </object>
    <object class="separator"/>
    <object class="wxMenuItem" name="ID_REMOVE">
      <label>Remove &amp;selected</label>
    </object>
    <object class="wxMenuItem" name="ID_REQUEUE">
      <label>Reset and requeue selected files</label>
    </object>
  </object>
  <object class="wxMenu" name="ID_MENU_QUEUE_SUCCESSFUL">
    <object class="wxMenuItem" name="ID_REMOVEALL">
      <label>Remove &amp;all</label>
    </object>
    <object class="separator"/>
    <object class="wxMenuItem" name="ID_REMOVE">
      <label>Remove &amp;selected</label>
    </object>
    <object class="wxMenuItem" name="ID_REQUEUE">
      <label>Reset and requeue selected files</label>
    </object>
    <object class="separator"/>
    <object class="wxMenuItem" name="ID_AUTOCLEAR">
      <label>&amp;Automatically remove successful transfers</label>
      <checkable>1</checkable>
    </object>
  </object>
  <object class="wxMenu" name="ID_MENU_REMOTETREE">
    <object class="wxMenuItem" name="ID_DOWNLOAD">
      <label>&amp;Download</label>
      <bitmap stock_id="ART_DOWNLOAD"></bitmap>
      <help>Download selected directory</help>
    </object>
