<?xml version="1.0" encoding="ISO-8859-1"?>
<resource version="2.3.0.1" xmlns="http://www.wxwindows.org/wxxrc">
  <object class="wxDialog" name="ID_VERIFYCERT">
    <title>Unknown certificate</title>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxStaticBitmap" name="ID_IMAGE">
              <bitmap stock_id="wxART_INFORMATION"></bitmap>
            </object>
            <flag>wxALL</flag>
            <border>7</border>
          </object>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <object class="sizeritem">
                <object class="wxStaticText" name="ID_DESC">
                  <label>The server's certificate is unknown. Please carefully examine the certificate to make sure the server can be trusted.</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticBoxSizer">
                  <label>Details</label>
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Host:</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_HOST">
                          <label>&lt;Host:port&gt;</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Valid from:</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_ACTIVATION_TIME">
                          <label></label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Valid to:</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_EXPIRATION_TIME">
                          <label></label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Serial number:</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_SERIAL">
                          <label></label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Public key algorithm:</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_PKALGO">
                          <label></label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Fingerprint (MD5):</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_FINGERPRINT_MD5">
                          <label></label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText">
                          <label>Fingerprint (SHA-1):</label>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_FINGERPRINT_SHA1">
                          <label></label>
                        </object>
                      </object>
                      <vgap>2</vgap>
                      <hgap>2</hgap>
                    </object>
                    <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxGROW</flag>
                    <border>4</border>
                  </object>
                </object>
                <flag>wxEXPAND</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticBoxSizer">
                  <label>Subject of certificate</label>
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_SUBJECT_DUMMY">
                          <label>&lt;Dummy&gt;</label>
                        </object>
                      </object>
                      <vgap>2</vgap>
                      <hgap>2</hgap>
                    </object>
                    <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxGROW</flag>
                    <border>4</border>
                  </object>
                </object>
                <flag>wxGROW</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticBoxSizer">
                  <label>Certificate issuer</label>
                  <orient>wxVERTICAL</orient>
                  <object class="sizeritem">
                    <object class="wxFlexGridSizer">
                      <cols>2</cols>
                      <object class="sizeritem">
                        <object class="wxStaticText" name="ID_ISSUER_DUMMY">
                          <label>&lt;Dummy&gt;</label>
                        </object>
                      </object>
                      <vgap>2</vgap>
                      <hgap>2</hgap>
                    </object>
                    <flag>wxBOTTOM|wxLEFT|wxRIGHT|wxGROW</flag>
                    <border>4</border>
                  </object>
                </object>
                <flag>wxGROW</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Trust this certificate and carry on connecting</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxCheckBox" name="ID_ALWAYS">
                  <label>&amp;Always trust certificate in future sessions.</label>
                </object>
              </object>
              <cols>1</cols>
              <vgap>5</vgap>
            </object>
            <flag>wxALL</flag>
            <border>5</border>
          </object>
        </object>
      </object>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_OK">
              <label>OK</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_CANCEL">
              <label>Cancel</label>
            </object>
          </object>
        </object>
        <flag>wxALIGN_CENTRE_HORIZONTAL</flag>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="ID_SETTINGS_TRANSFER">
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Concurrent transfers</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>3</cols>
              <vgap>4</vgap>
              <hgap>4</hgap>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Maximum simultaneous &amp;transfers:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_NUMTRANSFERS">
                  <size>40,-1</size>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>(1-10)</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Limit for concurrent &amp;downloads:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_NUMDOWNLOADS">
                  <size>40,-1</size>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>(0 for no limit)</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Limit for concurrent &amp;uploads:</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxTextCtrl" name="ID_NUMUPLOADS">
                  <size>40,-1</size>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>(0 for no limit)</label>
                </object>
                <flag>wxALIGN_CENTRE_VERTICAL</flag>
              </object>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
      <object class="sizeritem">
        <object class="wxStaticBoxSizer">
          <label>Timeout</label>
          <orient>wxVERTICAL</orient>
          <object class="sizeritem">
            <object class="wxFlexGridSizer">
              <cols>1</cols>
              <object class="sizeritem">
                <object class="wxFlexGridSizer">
                  <cols>3</cols>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>Time&amp;out in seconds:</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxTextCtrl" name="ID_TIMEOUT">
                      <size>40,-1</size>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                  <hgap>4</hgap>
                  <object class="sizeritem">
                    <object class="wxStaticText">
                      <label>(5-9999, 0 to disable)</label>
                    </object>
                    <flag>wxALIGN_CENTRE_VERTICAL</flag>
                  </object>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>If no data can be sent or received during this time, the connection will be closed and FileZilla will try to reconnect.</label>
                </object>
              </object>
              <vgap>4</vgap>
            </object>
            <flag>wxBOTTOM|wxLEFT|wxRIGHT</flag>
            <border>4</border>
          </object>
        </object>
        <flag>wxGROW</flag>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="ID_FILEEXISTSDLG">
    <style>wxCAPTION|wxSYSTEM_MENU|wxCLOSE_BOX</style>
    <exstyle>wxWS_EX_BLOCK_EVENTS</exstyle>
    <title>Target file already exists</title>
    <centered>1</centered>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <border>0</border>
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <border>0</border>
            <object class="wxBoxSizer">
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <flag>wxALIGN_LEFT|wxLEFT|wxRIGHT|wxTOP|wxADJUST_MINSIZE</flag>
                <border>5</border>
                <object class="wxStaticText" name="wxID_STATIC">
                  <label>The target file already exists.\nWould you like to replace the file</label>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxALIGN_LEFT|wxLEFT|wxRIGHT|wxTOP|wxADJUST_MINSIZE</flag>
