                          <checked>0</checked>
                        </object>
                      </object>
                    </object>
                  </object>
                </object>
              </object>
            </object>
          </object>
        </object>
      </object>
      <object class="sizeritem">
        <flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>
        <border>5</border>
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_OK">
              <label>OK</label>
              <default>1</default>
            </object>
          </object>
          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
            <border>5</border>
            <object class="wxButton" name="wxID_CANCEL">
              <label>&amp;Cancel</label>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
  <object class="wxDialog" name="ID_SITEMANAGER">
    <centered>1</centered>
    <style>wxCAPTION|wxSYSTEM_MENU|wxWS_EX_BLOCK_EVENTS|wxCLOSE_BOX</style>
    <exstyle>wxWS_EX_BLOCK_EVENTS</exstyle>
    <title>Site Manager</title>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <object class="wxBoxSizer">
          <orient>wxHORIZONTAL</orient>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <object class="wxStaticText">
                  <label>Select Entry:</label>
                </object>
              </object>
              <object class="sizeritem">
                <object class="wxTreeCtrl" name="ID_SITETREE">
                  <style>wxTR_HAS_BUTTONS|wxTR_EDIT_LABELS|wxSUNKEN_BORDER</style>
                </object>
                <option>1</option>
                <flag>wxTOP|wxBOTTOM|wxGROW</flag>
                <border>5</border>
              </object>
              <object class="sizeritem">
                <object class="wxGridSizer">
                  <cols>2</cols>
                  <rows>2</rows>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_NEWSITE">
                      <label>&amp;New Site</label>
                    </object>
                    <flag>wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_NEWFOLDER">
                      <label>New &amp;Folder</label>
                    </object>
                    <flag>wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_RENAME">
                      <label>&amp;Rename</label>
                    </object>
                    <flag>wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_DELETE">
                      <label>&amp;Delete</label>
                    </object>
                    <flag>wxGROW</flag>
                  </object>
                  <object class="sizeritem">
                    <object class="wxButton" name="ID_COPY">
                      <label>Cop&amp;y</label>
                    </object>
                    <flag>wxGROW</flag>
                  </object>
                  <vgap>5</vgap>
                  <hgap>5</hgap>
                </object>
                <flag></flag>
                <border>5</border>
              </object>
            </object>
            <flag>wxRIGHT|wxGROW</flag>
            <border>5</border>
          </object>
          <object class="sizeritem">
            <object class="wxBoxSizer">
              <orient>wxVERTICAL</orient>
              <object class="sizeritem">
                <object class="wxNotebook" name="ID_NOTEBOOK">
                  <object class="notebookpage">
                    <label>General</label>
                    <object class="wxPanel">
                      <object class="wxBoxSizer">
                        <orient>wxVERTICAL</orient>
                        <object class="sizeritem">
                          <object class="wxFlexGridSizer">
                            <cols>2</cols>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>&amp;Host:</label>
                              </object>
                              <flag>wxALIGN_CENTRE_VERTICAL</flag>
                            </object>
                            <object class="sizeritem">
                              <object class="wxBoxSizer">
                                <orient>wxHORIZONTAL</orient>
                                <object class="sizeritem">
                                  <object class="wxTextCtrl" name="ID_HOST"/>
                                  <option>1</option>
                                </object>
                                <object class="sizeritem">
                                  <object class="wxStaticText">
                                    <label>&amp;Port:</label>
                                  </object>
                                  <flag>wxLEFT|wxALIGN_CENTRE_VERTICAL</flag>
                                  <border>10</border>
                                </object>
                                <object class="sizeritem">
                                  <object class="wxTextCtrl" name="ID_PORT">
                                    <size>50,-1</size>
                                    <style></style>
                                  </object>
                                  <flag>wxLEFT</flag>
                                  <border>5</border>
                                </object>
                              </object>
                              <flag>wxGROW</flag>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>Server&amp;type:</label>
                              </object>
                              <flag>wxALL|wxALIGN_CENTRE_VERTICAL</flag>
                            </object>
                            <object class="sizeritem">
                              <object class="wxChoice" name="ID_PROTOCOL">
                                <content>
                                  <item>FTP</item>
                                  <item>SFTP</item>
                                </content>
                                <selection>0</selection>
                              </object>
                              <flag>wxGROW</flag>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticLine"/>
                              <flag>wxTOP|wxBOTTOM|wxGROW</flag>
                              <border>5</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticLine"/>
                              <flag>wxTOP|wxBOTTOM|wxGROW</flag>
                              <border>5</border>
                            </object>
                            <object class="sizeritem">
                              <object class="wxStaticText">
                                <label>&amp;Logontype:</label>
                              </object>
                              <flag>wxALIGN_CENTRE_VERTICAL</flag>
