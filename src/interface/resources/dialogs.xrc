                  <orient>wxHORIZONTAL</orient>
                  <object class="spacer">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                    <border>4</border>
                    <size>3,4</size>
                  </object>
                  <object class="sizeritem">
                    <flag>wxALIGN_CENTER_VERTICAL|wxLEFT|wxRIGHT</flag>
                    <border>5</border>
                    <object class="wxBoxSizer">
                      <orient>wxVERTICAL</orient>
                      <object class="sizeritem">
                        <flag>wxALIGN_LEFT|wxRIGHT|wxTOP</flag>
                        <border>5</border>
                        <object class="wxCheckBox" name="ID_QUEUEONLY">
                          <label>Apply to &amp;current queue only</label>
                          <checked>0</checked>
                        </object>
                      </object>
                      <object class="sizeritem">
                        <flag>wxALIGN_LEFT|wxRIGHT|wxTOP|wxBOTTOM</flag>
                        <border>5</border>
                        <object class="wxCheckBox" name="ID_UPDOWNONLY">
